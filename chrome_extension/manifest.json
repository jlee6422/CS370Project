{
    "manifest_version": 3,
    "name": "Website Blocker",
    "version": "1.0",
    "permissions": ["webRequest", "webRequestBlocking", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "*://*/*"
    ]
  }
  